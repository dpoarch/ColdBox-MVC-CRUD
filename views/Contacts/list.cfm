<cfoutput><div class="container"> <div class="row"><h1>Contact List</h1>#getInstance("MessageBox@cbMessageBox").renderit()#<p>	<a href="#event.buildLink('contacts.add')#" class="btn btn-success">Create Contact</a>	<a href="#event.buildLink('api.contacts')#" class="btn btn-warning">Generate JSON Data</a></p><table name="contactForm" border="1" width="98%" cellpadding="5" cellspacing="1">	<thead>		<th>ID</th>		<th>Name</th>		<th>Email</th>		<th width="75">CMDS</th>	</thead>	<tbody>		<cfloop query="prc.qContacts">			<tr>				<td>#prc.qContacts.id#</td>				<td>#prc.qContacts.name#</td>				<td>#prc.qContacts.email#</td>				<td>					<a href="#event.buildLink('contacts.delete')#/id/#prc.qContacts.id#" onclick="return confirm('Are you sure?')">Delete</a>				</td>			</tr>		</cfloop>	</tbody></table></div></div></cfoutput>