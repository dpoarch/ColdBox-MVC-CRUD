<cfoutput><div class="container"><div class="row"><h1>Create New Contact</h1>#getInstance("MessageBox@cbMessageBox").renderit()#<form name="addContactForm" method="post" action="#event.buildLink('contacts.createContact')#">	<label>Name:<br> 		<input type="text" name="name" size="30" class="form-control"/>	</label>	<br/>		<label>Email: <br>		<input type="text" name="email" size="30" class="form-control" />	</label>		<br/><br/>		<a href="#event.buildLink('contacts')#" class="btn btn-danger">Cancel</a>	<input type="submit" value="Create Contact!" class="btn btn-success" /></form></div></div></cfoutput>